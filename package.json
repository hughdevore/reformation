    "redraft": "0.10.2",
    "source-map-support": "^0.5.10",
    "styled-theming": "2.2.0"
  },
  "scripts": {
    "test": "env-cmd dev BABEL_ENV=test kyt test -- --forceExit --detectOpenHandles",
    "test-watch": "env-cmd dev BABEL_ENV=test kyt test -- --watch --verbose false",
    "test-update": "BABEL_ENV=test kyt test -- -u",
    "dev": "env-cmd dev kyt dev -C kyt.dev.js",
    "prod:build": "NODE_ENV=production env-cmd prod kyt build -C kyt.prod.js",
    "prod:stop": "forever stop highforthis",
    "prod:start": "NODE_ENV=production env-cmd prod forever start -a forever.prod.json",
    "prod": "npm run prod:build && npm run prod:stop && npm run prod:start",
    "prod:local": "yarn install && yarn prod:build && yarn prod:local:start",
    "prod:local:start": "env-cmd prod node build/server/main.js",
    "lint-fix": "eslint . --fix",
    "precommit": "eslint ."
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-decorators": "7.2.3",
    "@babel/preset-flow": "7.0.0",
