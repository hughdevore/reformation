    "enzyme-to-json": "3.3.5",
    "eslint-import-resolver-webpack": "^0.11.0",
    "flow-bin": "0.93.0",
    "husky": "1.3.1",
    "jest-emotion": "9.2.11",
    "kyt": "1.0.0-alpha.43822f6a",
    "raf": "3.4.1",
    "rimraf": "2.6.3"
  }
